{
  "name": "zkcloudworker-tests",
  "version": "0.1.0",
  "author": "DFST",
  "type": "module",
  "scripts": {
    "test": "jest --module=nodenext",
    "tx": "jest zktx",
    "prisma:generate": "yarn prisma generate",
    "prisma:seed": "yarn prisma db seed",
    "prisma:migrate": "yarn prisma migrate dev --name v1",
    "prisma:studio": "yarn prisma studio",
    "prisma:push": "yarn prisma db push",
    "prisma:migrate:deploy": "yarn prisma migrate deploy"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "jest": "^29.7.0",
    "prisma": "^6.2.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "packageManager": "yarn@4.5.1",
  "dependencies": {
    "@minatokens/storage": "^0.15.2",
    "@prisma/client": "^6.2.1",
    "axios": "^1.7.9",
    "jose": "^5.9.6",
    "js-sha256": "^0.11.0",
    "matrix-js-sdk": "^36.0.0",
    "nanoid": "^5.0.9",
    "nats": "^2.29.1",
    "o1js": "2.2.0",
    "openai": "^4.79.1",
    "protobufjs": "^7.4.0",
    "puppeteer": "^24.1.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "rss-parser": "^3.13.0",
    "zkcloudworker": "^0.23.14"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "slowTestThreshold": 1500,
    "testTimeout": 10800000,
    "bail": true,
    "verbose": true,
    "roots": [
      "tests"
    ],
    "setupFilesAfterEnv": [
      "./jest-config.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    }
  }
}
